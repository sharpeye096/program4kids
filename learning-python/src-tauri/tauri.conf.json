{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "learning-python",
  "version": "0.1.0",
  "identifier": "com.learning-python.app",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://localhost:8000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Learning Python",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}